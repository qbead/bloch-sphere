{
  "name": "@qbead/bloch-sphere",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "version": "1.1.4",
  "type": "module",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "browser": "./dist/index.js",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./src/index.d.ts",
      "default": "./dist/index.js"
